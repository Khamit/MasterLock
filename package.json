{
  "name": "masterlock",
  "displayName": "MasterLock",
  "description": "Opensource git -  | Hides sensitive data in the project and protects it with a system password.",
  "version": "1.0.8",
  "publisher": "Khamit",
  "engines": {
    "vscode": "^1.104.0"
  },
  "icon": "resources/logo.svg",
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:masterlock.toggleSelection"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "contributes": {
    "commands": [
      {
        "command": "masterlock.toggleSelection",
        "title": "MasterLock encrypt"
      },
      {
      "command": "masterlock.showLogo",
      "title": "MasterLock: Show Logo"
    }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "masterlock.toggleSelection",
          "group": "navigation",
          "when": "editorHasSelection && !masterlock.isEncrypted",
          "icon": {
            "light": "resources/light/unlock.svg",
            "dark": "resources/dark/unlock.svg"
          },
          "title": "MasterUnlocked"
        },
        {
          "command": "masterlock.toggleSelection",
          "group": "navigation",
          "when": "editorHasSelection && masterlock.isEncrypted",
          "icon": {
            "light": "resources/light/lock.svg",
            "dark": "resources/dark/lock.svg"
          },
          "title": "MasterLocked"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.19.19",
    "@types/vscode": "^1.104.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "keytar": "^7.9.0"
  },
    "repository": {
    "type": "git",
    "url": "https://github.com/Khamit/MasterLock.git"
  }
}
